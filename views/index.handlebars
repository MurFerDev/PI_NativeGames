<body>
    {{>jumbotron}}
    {{>globe}}

    <main>
        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">

        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="3" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="4" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="5" class="active"></li>
        </ol>

        <div class="bd-example">
            <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="w-100"
                            data-src="holder.js/525x300?auto=yes&amp;bg=777&amp;fg=555&amp;text=Adugo"
                            alt="Adugo [525x300]" src="/static/images/adugo.png" data-holder-rendered="true">
                            <a href="/adugo"></a>
                        </div>
                        <div class="carousel-item">
                            <img class="w-100"
                            data-src="holder.js/525x300?auto=yes&amp;bg=666&amp;fg=444&amp;text=Dou-Shou-Qi"
                            alt="Dou-Shou-Qi [525x300]" src="/static/images/doushouqi.png" data-holder-rendered="true">
                            <a href="/doushouqi"></a>
                    </div>
                    <div class="carousel-item">
                        <img class="w-100"
                            data-src="holder.js/525x300?auto=yes&amp;bg=555&amp;fg=333&amp;text=Bagha-Chall"
                            alt="Bagha-Chall [525x300]" src="/static/images/baghachall.png" data-holder-rendered="true">
                            <a href="/baghachall"></a>
                    </div>
                    <div class="carousel-item">
                        <img class="w-100"
                            data-src="holder.js/525x300?auto=yes&amp;bg=555&amp;fg=333&amp;text=Senet"
                            alt="Senet [525x300]" src="/static/images/senet.png" data-holder-rendered="true">
                            <a href="/senet"></a>
                    </div>
                    <div class="carousel-item">
